{
  "name": "turbo-template",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.0",
    "eslint-custom": "workspace:*",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^15.0.2",
    "prettier": "^2.8.8",
    "simple-git-hooks": "^2.9.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "preserveUnused": true
  },
  "pnpm": {
    "overrides": {
      "glob": "^10.3.10",
      "rimraf": "^5.0.5"
    }
  }
}
